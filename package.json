{
  "name": "contempo-node",
  "version": "1.0.0",
  "description": "Just another node API boilerplate",
  "main": "index.js",
  "repository": "https://github.com/therealedsheenan/contempo-node.git",
  "author": "therealedsheenan <private.sheenantenepre@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.17.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "nodemon": "^1.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.5",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^4.11.8",
    "mongoose-unique-validator": "^1.0.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "build": "babel src -s -D -d dist --presets es2015",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015\"",
